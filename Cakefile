require "coffee-script/register"

task 'bdd:verify', 'compile a test for manual verification. arguments: TEST_NAME', (options) ->



task 'bdd:approve', 'manually approve a bdd test. arguments: TEST_NAME', (options) ->
  bdd = require "./test/bdd/bdd"
  FunCSS = require "./src/funcss"
  fs = require "fs"

  
option '-t', '--test <test name>'
option '-m', '--message <message>'
task 'bdd:fail', 'manually mark a bdd test as failed', (options) ->
  bdd = require "./test/bdd/bdd.coffee"
  FunCSS = require "./src/funcss"
  fs = require "fs"

  
